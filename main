-- [[ L√ìGICA DE NAVEGA√á√ÉO ]] --
local function openTab(page, btn)
    PagePlayer.Visible = false; PageServers.Visible = false; PageFly.Visible = false; PageHelper.Visible = false
    btnPlayer.BackgroundColor3 = COLORS.BTN; btnServers.BackgroundColor3 = COLORS.BTN; btnFly.BackgroundColor3 = COLORS.BTN; btnHelper.BackgroundColor3 = COLORS.BTN
    btnPlayer.TextColor3 = COLORS.TEXT; btnServers.TextColor3 = COLORS.TEXT; btnFly.TextColor3 = COLORS.TEXT; btnHelper.TextColor3 = COLORS.TEXT
    
    page.Visible = true
    btn.BackgroundColor3 = COLORS.ACCENT
    btn.TextColor3 = COLORS.BG
end

btnPlayer.MouseButton1Click:Connect(function() openTab(PagePlayer, btnPlayer) end)
btnServers.MouseButton1Click:Connect(function() openTab(PageServers, btnServers) end)
btnFly.MouseButton1Click:Connect(function() openTab(PageFly, btnFly) end)
btnHelper.MouseButton1Click:Connect(function() openTab(PageHelper, btnHelper) end)

